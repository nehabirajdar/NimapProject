@model IEnumerable<NimapProject.Models.Category>
@{
    ViewData["Title"] = "List";
}
@Html.ActionLink("Add Category","AddCategory")

<table class="table">
    <tr>
        <th>Category Id</th>
        <th>Category Name</th>
        <th>Actions</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CategoryId</td>
            <td>@item.CategoryName</td>
            <td>
                @Html.ActionLink("Edit","EditCategory",new {id=item.CategoryId}) |
                @Html.ActionLink("Delete","DeleteCategory",new {id=item.CategoryId})
            </td>
        </tr>
    }
  
</table>